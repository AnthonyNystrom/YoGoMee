<Window x:Class="YGMEUserContactAccessApp.LoginWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Login" Height="175" Width="300">
    <Grid>
        <Button Height="23" HorizontalAlignment="Right" Margin="0,0,18,12" Name="button1" VerticalAlignment="Bottom" Width="75" Click="button1_Click">Cancel</Button>
        <TextBox Height="23" Margin="112,17,20,0" Name="textBox1" VerticalAlignment="Top" />
        <PasswordBox Height="23" Margin="112,61,20,0" Name="passwordBox1" VerticalAlignment="Top" />
        <Label Height="28" Margin="12,17,0,0" Name="label1" VerticalAlignment="Top" HorizontalAlignment="Left" Width="120">User Id</Label>
        <Label Margin="12,55,0,0" Name="label2" HorizontalAlignment="Left" Width="120" Height="26" VerticalAlignment="Top">Password</Label>
        <Button Height="23" Margin="98,0,105,12" Name="button2" VerticalAlignment="Bottom" Click="button2_Click">Login</Button>
    </Grid>
</Window>
